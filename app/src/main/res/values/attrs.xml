<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyTestView">
        <attr name="text_value" format="string"/>
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="TextDrawableView">

        <attr name="textdrawable_value" format="string"/>
        <attr name="textdrawable_color" format="color"/>
        <attr name="textdrawable_size" format="dimension"/>
        <attr name="textdrawable_img" format="reference"/>

    </declare-styleable>


    <declare-styleable name="CircleProgressView">

        <attr name="progress_bg_colcor" format="color"/>
        <attr name="progress_color" format="color"/>
        <attr name="progress_text_size" format="dimension"/>
        <attr name="progress_text_color" format="color"/>
        <attr name="progress" format="integer"/>
        <attr name="progress_width" format="dimension"/>
        <attr name="max_progress" format="integer"/>


    </declare-styleable>


    <declare-styleable name="VolumeView">

        <attr name="volume_count" format="integer"/>
        <attr name="volume_color" format="color"/>
        <attr name="volume_bg_color" format="color"/>
        <attr name="volume_width" format="dimension"/>
        <attr name="volume_spacing" format="dimension"/>
        <attr name="volume_bitmap" format="reference"/>


    </declare-styleable>

    <declare-styleable name="VolumeHorizontalView">
        <attr name="volume_horizontal_totalProgress" format="integer"/>
        <attr name="volume_horizontal_bgColor" format="color"/>
        <attr name="volume_horizontal_color" format="color"/>
        <attr name="volume_horizontal_bitmap" format="reference"/>
        <attr name="volume_horizontal_height" format="reference"/>
    </declare-styleable>

</resources>